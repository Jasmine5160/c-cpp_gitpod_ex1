#include <iostream>
#include <stdio.h>
using namespace std;
int main(){
   int x;
   int y;// int = 宣告
   int bool_value;
   
   printf("please input x:\n");// \\ \n 使句子換行 
    scanf("%d",&x);// %d =一個整數
   printf("please input y:\n");
    scanf("%d",&y);
    
    if(x > y){
    printf(" x > y ==> %d \n",x > y);
    }else if(x <= y){
    printf(" x > y ==> %d \n",x > y);
    }else{
    printf(" x == y  \n");
    }
    return 0;
}
