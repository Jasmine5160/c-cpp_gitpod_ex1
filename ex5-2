#include <iostream>
using namespace std;
int main()
{
 
   int i, x;
    
    for( i=1 ; i<10 ; i++ ){
        
		for( x=1 ; x<10 ; x++ ){
           // cout << i << "*" << x << "=" << i*x << " ";
            //printf("%9d \n");
            printf(" %d * %d = %d \n",i,x,i*x);
            
		}
    }
		
		cout << "Hello World" <<endl;
    return 0;
}
